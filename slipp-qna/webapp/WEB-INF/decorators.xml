<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE decorators>
<decorators defaultdir="/WEB-INF/jsp/decorators">
	<excludes>
		<pattern>/admin/h2/*</pattern>
	</excludes>

	<decorator name="bbs" page="defaultbbs.jsp">
		<pattern>/bbs/*</pattern>
	</decorator>
	
	<decorator name="bbsajax" page="defaultbbsajax.jsp">
		<pattern>/bbsajax/*</pattern>
	</decorator>	

	<decorator name="default" page="default.jsp">
		<pattern>*</pattern>
	</decorator>	
	
</decorators>